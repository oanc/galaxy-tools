            </entry>
            <entry name="getLogicalPeopleGroup">
               <entry name="Class" value="org.activebpel.rt.b4p.validators.AeHtFunctionValidator"/>
            </entry>
         </entry>
         <entry name="http://www.example.org/BPEL4People">
            <entry name="getLogicalPeopleGroup">
