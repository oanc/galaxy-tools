         <entry name="Namespace" value="http://www.example.org/BPEL4People"/>
         <entry name="LocalName" value="humanInteractions"/>
         <entry name="ClassName"
                value="org.activebpel.rt.b4p.def.ext.AeB4PHumanInteractionsExtensionObject"/>
