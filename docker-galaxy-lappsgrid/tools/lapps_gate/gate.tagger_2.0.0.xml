<tool id="gate.tagger_2.0.0" name="GATE POSTagger v2.0.0" version="2.0.0">
	<description>
		GATE Part of Speech Tagger
	</description>
	<command interpreter="lsd">
		invoke.lsd gate.tagger_2.0.0 $input $username $password $output
	</command>
	<inputs>
		<param name="input" type="data" format="gate" label="input"/>
		<param name="username" type="text" value="tester" label="Username"/>
		<param name="password" type="text" value="tester" label="Password"/>
	</inputs>
	<outputs>
		<data name="output" format="gate" label="Tagged (GATE)"/>
	</outputs>
	<help>
		GATE Part of Speech Tagger
	</help>
</tool>
